node ('master'){
    stage('Preparation') {
       checkout scm
    }
    stage('Build') {
        withMaven(globalMavenSettingsConfig: '', jdk: '', maven: 'Default', mavenSettingsConfig: '', traceability: true) {
            sh 'mvn clean test -Dtype.browser=${browser}'
        }
    }
    stage('Results') {
        allure includeProperties: false, jdk: '', results: [[path: 'target/allure-results']]
    }
}
